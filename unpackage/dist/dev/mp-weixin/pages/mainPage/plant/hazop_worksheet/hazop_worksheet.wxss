.table {
    border: 0px solid darkgray;
    font-size: 12px;
}
.tr {
    display: flex;
    width: 100%;
    align-items: center;
}
.td {
    width: 14%;
    min-height: 2rem;
    justify-content: center;
    text-align: center;
    word-wrap: break-word;
    flex-shrink: 0;
    line-height: 2rem;
}
.gray {
    background: white;
}

